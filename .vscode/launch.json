{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        
        {
            "name": "Launch Elo web service",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "./elo-web-service",
            "args": [
                "--google-service-account-key", "../google-service-account-key.json",
                "--doc-id", "1bf6bmd63dvO9xjtnoTGmkcWJJE0NetQRjKkgcQvovQQ",
                "--address", "localhost:8080"
            ]
        },
        {
            "name": "Next.js: debug full stack",
            "type": "node",
            "request": "launch",
            "program": "${workspaceFolder}/nextjs/node_modules/next/dist/bin/next",
            "runtimeArgs": ["--inspect"],
            "skipFiles": ["<node_internals>/**"],
            "cwd": "${workspaceFolder}/nextjs",
            "serverReadyAction": {
                "action": "debugWithChrome",
                "killOnServerStop": true,
                "pattern": "- Local:.+(https?://.+)",
                "uriFormat": "%s",
                "webRoot": "${workspaceFolder}"
            }
        }
    ]
}